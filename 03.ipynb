   ],
   "source": [
    "#Import Math Module\n",
    "import math\n",
    "\n",
    "d=12\n",
    "r=d/2\n",
    "V= (4/3)*(math.pi)*(r**3)\n",
    "print(\"Output:\")\n",
    "print(\"The Volume of sphere is :\"+str(V)+\" cc\")"
